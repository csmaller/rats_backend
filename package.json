{
  "name": "hello-prisma",
  "license": "MIT",
  "scripts": {
    "queries": "ts-node ./src/queries.ts",
    "caching": "ts-node ./src/caching.ts",
    "seed": "ts-node ./src/seed.ts",
    "test-seed": "ts-node ./src/seed.test.ts",
    "start": "node ./scripts/check-node.js && ts-node src/server.ts",
    "start:dev": "ts-node src/server.ts",
    "start:prod": "node ./scripts/check-node.js && npx tsc && node dist/src/server.js",
    "api-test": "ts-node src/api.test.ts",
    "dev:test": "wait-on http://localhost:4000 && npm run api-test",
    "dev": "concurrently \"npm: start:dev\" \"npm:dev:test\"",
    "build": "npx prisma generate && npx tsc"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@prisma/extension-accelerate": "2.0.2",
    "@prisma/extension-optimize": "^2.0.0",
    "@types/node": "22.15.32",
    "cors": "2.8.5",
    "dotenv": "16.6.1",
    "express": "4.18.2",
    "undici": "5.12.0"
  },
  "devDependencies": {
    "@netlify/functions": "1.4.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "concurrently": "8.2.0",
    "prisma": "^6.16.3",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "wait-on": "7.0.1"
  }
}
